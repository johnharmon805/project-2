  <!--Nav Bar-->
    <div class="row">
        <div class="col-md-12">
            <nav class="navbar navbar-expand-lg navbar-light bg-light">
                <img id="logo" src="styles/Images/Inline-Logo.png">
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
              <span class="navbar-toggler-icon"></span>
            </button>
                <div class="collapse navbar-collapse" id="navbarNav">
                    <ul class="navbar-nav">
                        <li class="nav-item active">
                            <a class="nav-link" href="/">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="/shop">Shop<span class="sr-only">(current)</span></a>
                        </li>
                        <li class="nav-item">
                            <a id="view-cart" class="nav-link" href="/cart">View Cart</a>
                        </li>
                    </ul>
                </div>
            </nav>
        </div>
    </div>

    <div class="row">
        <div class="col-md-4"></div>
        <div class="col-md-4"> 
        <ul id= "productId">
            {{#each Products}}
            <li id={{this.dataValues.id}} value="{{this.dataValues.product_name}}" price={{this.dataValues.price}} image="/styles/Images/{{this.dataValues.image_url}}">
            <img src="/styles/Images/{{this.dataValues.image_url}}" class="shopImages"></img>
            <a href="/product/"{{this.dataValues.id}}><button type="button" id="viewdetails" class="btn btn-dark">View Details</button></a>
            <p id = "shop-name">{{this.dataValues.product_name}}</p>
            <p id = "shop-price">${{this.dataValues.price}}.00</p>
            <button type="button" id="shop-add-to-cart" id={{this.dataValues.id}} class="btn btn-success">Add to Cart</button>
            </li>
            {{/each}}
        </ul>
        </div>
    </div>
    <div class="col-md-4"></div>
</div>

